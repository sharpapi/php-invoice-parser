{
  "name": "sharpapi/php-invoice-parser",
  "description": "Pure PHP client for SharpAPI Invoice Parser (extracts structured data from invoices in PDF/TIFF/JPG/PNG).",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "SharpAPI",
    "invoice",
    "parser",
    "finance",
    "ocr",
    "php"
  ],
  "homepage": "https://sharpapi.com/",
  "authors": [
    {
      "name": "A2Z WEB LTD",
      "homepage": "https://sharpapi.com/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "sharpapi/php-core": "^1.4.0",
    "guzzlehttp/guzzle": "^7.0|^7.9|^7.10"
  },
  "autoload": {
    "psr-4": {
      "SharpAPI\\FinanceParseInvoice\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "source": "https://github.com/sharpapi/php-invoice-parser",
    "issues": "https://github.com/sharpapi/php-invoice-parser/issues"
  }
}
